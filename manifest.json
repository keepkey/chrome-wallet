{
  "name": "KeepKey Launcher",
  "description": "Launch the KeepKey wallet application",
  "version": "3.1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "assets/icons/128x128.png",
    "default_title": "KeepKey Wallet"
  },
  "icons": {
    "16": "assets/icons/16x16.png",
    "48": "assets/icons/48x48.png",
    "128": "assets/icons/128x128.png"
  },
  "permissions": [
    "management"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
